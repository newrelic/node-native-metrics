{
  "name": "@newrelic/native-metrics",
  "version": "1.0.0",
  "description": "A module for generating metrics from V8.",
  "main": "index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "lint": "eslint ./*.js lib",
    "unit": "tap --expose-gc tests/**/*.tap.js",
    "integration": "node tests/integration/server.js",
    "test": "npm run unit && npm run integration",
    "publish-binary": "node-pre-gyp --acl='' unpublish package publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:newrelic/node-native-metrics"
  },
  "keywords": [
    "newrelic",
    "gc",
    "metrics",
    "stats",
    "gc-stats",
    "gc stats",
    "gc metrics",
    "native-metrics",
    "native metrics"
  ],
  "author": "New Relic Node.js agent team <nodejs@newrelic.com>",
  "contributors": [
    {
      "name": "Natalie Wolfe",
      "email": "nwolfe@newrelic.com",
      "web": "https://newrelic.com"
    }
  ],
  "license": "LicenseRef-LICENSE",
  "binary": {
    "module_name": "native_metrics",
    "module_path": "./lib/binary",
    "host": "https://nr-downloads-main.s3.amazonaws.com",
    "remote_path": "./nodejs_agent/builds"
  },
  "engines": {
    "node": ">=0.12",
    "npm": ">=2"
  },
  "devDependencies": {
    "async": "^2.1.4",
    "aws-sdk": "^2.7.0",
    "eslint": "^3.9.1",
    "segfault-handler": "^1.0.0",
    "semver": "^5.3.0",
    "tap": "^8.0.0"
  },
  "dependencies": {
    "nan": "^2.4.0",
    "node-pre-gyp": "^0.6.31"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ]
}
